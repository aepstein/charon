<h1>Listing agreement_criterions</h1>

<table>
  <tr>
    <th>Agreement</th>
  </tr>

<% @agreement_criterions.each do |agreement_criterion| %>
  <tr>
    <td><%=h agreement_criterion.agreement %></td>
    <td><%= link_to 'Show', agreement_criterion %></td>
    <td><%= link_to 'Edit', edit_agreement_criterion_path(agreement_criterion) %></td>
    <td><%= link_to 'Destroy', agreement_criterion, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New agreement_criterion', new_agreement_criterion_path %>