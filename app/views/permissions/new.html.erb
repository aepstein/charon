<h1>New permission in <%=h @permission.framework %></h1>

<% form_for(@permission) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'form', :object => f %>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<p>
<%= link_to 'Back', framework_permissions_path( @permission.framework ) %>
</p>

