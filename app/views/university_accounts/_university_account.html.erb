<% organization ||= false %>
  <%= table_row_tag do %>
    <% unless organization -%>
    <td><%=h university_account.organization %></td>
    <% end -%>
    <td><%=h university_account.department_code %></td>
    <td><%=h university_account.subledger_code %></td>
    <td><%= link_to 'Show', university_account %></td>
    <td><%= link_to( 'Edit', edit_university_account_path(university_account) ) if permitted_to? :edit, university_account %></td>
    <td><%= link_to( 'Destroy', university_account, :confirm => 'Are you sure?', :method => :delete ) if permitted_to? :destroy, university_account %></td>
  <% end %>

