<h1>Editing version</h1>

<% form_for(@version) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => "form",
             :object => f %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @version %> |
<%= link_to 'Back', request_items_path(@version.item.request) %>

