<h1>Editing fulfillment</h1>

<% form_for(@fulfillment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :fulfiller %><br />
    <%= f.text_field :fulfiller %>
  </p>
  <p>
    <%= f.label :fulfiller_type %><br />
    <%= f.text_field :fulfiller_type %>
  </p>
  <p>
    <%= f.label :fulfilled %><br />
    <%= f.text_field :fulfilled %>
  </p>
  <p>
    <%= f.label :fulfilled_type %><br />
    <%= f.text_area :fulfilled_type %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @fulfillment %> |
<%= link_to 'Back', fulfillments_path %>